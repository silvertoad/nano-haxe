<?xml version="1.0" encoding="utf-8"?>
<project>

    <app title="nano test" main="NanoTest" file="test" package="me.silvertoad.nano.haxe" version="1.0.0" company="CrabsCrew"/>

    <window width="800" height="600" fps="30" orientation="portrait" resizable="true"/>

    <set name="BUILD_DIR" value="../generate/nano_test"/>
    <classpath name="src"/>
    <classpath name="../src"/>

    <haxelib name="nme"/>

    <section if="flash">
        <set name="SWF_VERSION" value="10.1" />
        <haxedef name="fdb"/>
    </section>

    <assets path="assets" include="*"/>

    <ndll name="std"/>
    <ndll name="regexp"/>
    <ndll name="zlib"/>
    <ndll name="nme" haxelib="nme"/>
</project>